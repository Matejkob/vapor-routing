{"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/orderedcollections\/ordereddictionary\/subscript(_:default:)"]}],"sections":[],"abstract":[{"text":"Accesses the value with the given key. If the dictionary doesn’t contain","type":"text"},{"text":" ","type":"text"},{"text":"the given key, accesses the provided default value as if the key and","type":"text"},{"text":" ","type":"text"},{"text":"default value existed in the dictionary.","type":"text"}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/co.pointfree.VaporRouting\/documentation\/OrderedCollections\/OrderedDictionary\/subscript(_:default:)"},"hierarchy":{"paths":[["doc:\/\/co.pointfree.VaporRouting\/documentation\/OrderedCollections","doc:\/\/co.pointfree.VaporRouting\/documentation\/OrderedCollections\/OrderedDictionary"]]},"primaryContentSections":[{"declarations":[{"platforms":["Linux"],"languages":["swift"],"tokens":[{"text":"subscript","kind":"keyword"},{"text":"(","kind":"text"},{"text":"key","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"Key","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"default","kind":"externalParam"},{"text":" ","kind":"text"},{"text":"defaultValue","kind":"internalParam"},{"text":": ","kind":"text"},{"text":"@autoclosure ","kind":"attribute"},{"text":"() -> ","kind":"text"},{"text":"Value","kind":"typeIdentifier"},{"text":") -> ","kind":"text"},{"text":"Value","kind":"typeIdentifier"},{"text":" { ","kind":"text"},{"text":"get","kind":"keyword"},{"text":" ","kind":"text"},{"text":"set","kind":"keyword"},{"text":" }","kind":"text"}]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"key","content":[{"type":"paragraph","inlineContent":[{"text":"The key the look up in the dictionary.","type":"text"}]}]},{"name":"defaultValue","content":[{"type":"paragraph","inlineContent":[{"text":"The default value to use if ","type":"text"},{"code":"key","type":"codeVoice"},{"text":" doesn’t exist in the","type":"text"},{"text":" ","type":"text"},{"text":"dictionary.","type":"text"}]}]}]},{"content":[{"type":"heading","anchor":"return-value","text":"Return Value","level":2},{"type":"paragraph","inlineContent":[{"text":"The value associated with ","type":"text"},{"code":"key","type":"codeVoice"},{"text":" in the dictionary; otherwise,","type":"text"},{"text":" ","type":"text"},{"code":"defaultValue","type":"codeVoice"},{"text":".","type":"text"}]}],"kind":"content"},{"content":[{"anchor":"overview","text":"Overview","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"text":"Use this subscript when you want either the value for a particular key","type":"text"},{"text":" ","type":"text"},{"text":"or, when that key is not present in the dictionary, a default value. This","type":"text"},{"text":" ","type":"text"},{"text":"example uses the subscript with a message to use in case an HTTP response","type":"text"},{"text":" ","type":"text"},{"text":"code isn’t recognized:","type":"text"}]},{"code":["var responseMessages: OrderedDictionary = [","    200: \"OK\",","    403: \"Access forbidden\",","    404: \"File not found\",","    500: \"Internal server error\"]","","let httpResponseCodes = [200, 403, 301]","for code in httpResponseCodes {","    let message = responseMessages[code, default: \"Unknown response\"]","    print(\"Response \\(code): \\(message)\")","}","\/\/ Prints \"Response 200: OK\"","\/\/ Prints \"Response 403: Access forbidden\"","\/\/ Prints \"Response 301: Unknown response\""],"type":"codeListing","syntax":null},{"type":"paragraph","inlineContent":[{"text":"When a dictionary’s ","type":"text"},{"code":"Value","type":"codeVoice"},{"text":" type has value semantics, you can use this","type":"text"},{"text":" ","type":"text"},{"text":"subscript to perform in-place operations on values in the dictionary.","type":"text"},{"text":" ","type":"text"},{"text":"The following example uses this subscript while counting the occurrences","type":"text"},{"text":" ","type":"text"},{"text":"of each letter in a string:","type":"text"}]},{"code":["let message = \"Hello, Elle!\"","var letterCounts: [Character: Int] = [:]","for letter in message {","    letterCounts[letter, default: 0] += 1","}","\/\/ letterCounts == [\"H\": 1, \"e\": 2, \"l\": 4, \"o\": 1, ...]"],"type":"codeListing","syntax":null},{"type":"paragraph","inlineContent":[{"text":"When ","type":"text"},{"type":"codeVoice","code":"letterCounts[letter, defaultValue: 0] += 1"},{"text":" is executed with a","type":"text"},{"text":" ","type":"text"},{"text":"value of ","type":"text"},{"type":"codeVoice","code":"letter"},{"text":" that isn’t already a key in ","type":"text"},{"type":"codeVoice","code":"letterCounts"},{"text":", the","type":"text"},{"text":" ","type":"text"},{"text":"specified default value (","type":"text"},{"type":"codeVoice","code":"0"},{"text":") is returned from the subscript,","type":"text"},{"text":" ","type":"text"},{"text":"incremented, and then added to the dictionary under that key.","type":"text"}]},{"type":"aside","style":"note","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Do not use this subscript to modify dictionary values if the"},{"type":"text","text":" "},{"type":"text","text":"dictionary’s "},{"type":"codeVoice","code":"Value"},{"type":"text","text":" type is a class. In that case, the default value"},{"type":"text","text":" "},{"type":"text","text":"and key are not written back to the dictionary after an operation. (For"},{"type":"text","text":" "},{"type":"text","text":"a variant of this operation that supports this usecase, see"},{"type":"text","text":" "},{"type":"codeVoice","code":"updateValue(forKey:default:_:)"},{"type":"text","text":".)"}]}],"name":"Note"},{"type":"aside","style":"note","content":[{"inlineContent":[{"text":"Looking up values in the dictionary through this subscript","type":"text"},{"text":" ","type":"text"},{"text":"has an expected complexity of O(1) hashing\/comparison operations on","type":"text"},{"text":" ","type":"text"},{"text":"average, if ","type":"text"},{"code":"Key","type":"codeVoice"},{"text":" implements high-quality hashing. Updating the","type":"text"},{"text":" ","type":"text"},{"text":"dictionary also has an amortized expected complexity of O(1) –","type":"text"},{"text":" ","type":"text"},{"text":"although individual updates may need to copy or resize the dictionary’s","type":"text"},{"text":" ","type":"text"},{"text":"underlying storage.","type":"text"}],"type":"paragraph"}],"name":"Complexity"}],"kind":"content"}],"metadata":{"extendedModule":"OrderedCollections","role":"symbol","modules":[{"name":"OrderedCollections"}],"title":"subscript(_:default:)","conformance":{"constraints":[{"code":"Key","type":"codeVoice"},{"text":" conforms to ","type":"text"},{"code":"Hashable","type":"codeVoice"},{"text":".","type":"text"}],"availabilityPrefix":[{"text":"Available when","type":"text"}],"conformancePrefix":[{"text":"Conforms when","type":"text"}]},"symbolKind":"subscript","fragments":[{"text":"subscript","kind":"keyword"},{"text":"(","kind":"text"},{"text":"Key","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"default","kind":"externalParam"},{"text":" _: () -> ","kind":"text"},{"text":"Value","kind":"typeIdentifier"},{"text":") -> ","kind":"text"},{"text":"Value","kind":"typeIdentifier"}],"roleHeading":"Instance Subscript","externalID":"s:18OrderedCollections0A10DictionaryV_7defaultq_x_q_yXKtcip"},"references":{"doc://co.pointfree.VaporRouting/documentation/OrderedCollections/OrderedDictionary":{"identifier":"doc:\/\/co.pointfree.VaporRouting\/documentation\/OrderedCollections\/OrderedDictionary","url":"\/documentation\/orderedcollections\/ordereddictionary","abstract":[{"text":"An ordered collection of key-value pairs.","type":"text"}],"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"OrderedDictionary","kind":"identifier"}]